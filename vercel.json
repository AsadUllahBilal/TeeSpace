{
  "buildCommand": "NEXT_TELEMETRY_DISABLED=1 SKIP_ENV_VALIDATION=1 next build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "SKIP_ENV_VALIDATION": "1",
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}
